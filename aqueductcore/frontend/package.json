{
  "name": "aqueduct-core",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testTimeout=10000",
    "test:coverage": "react-scripts test --coverage ./src --watchAll=false",
    "test:watch": "react-scripts test --watchAll",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "commit": "cz",
    "codegen": "graphql-codegen --config codegen.ts",
    "nuke": "rm package-lock.json && rm yarn.lock && rm -rf node_modules && rm -rf coverage && rm -rf build",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "format": "yarn format:eslint && yarn format:prettier",
    "lint:eslint": "eslint --ext .js,.ts,.tsx,.html src/",
    "format:eslint": "eslint --ext .js,.ts,.tsx,.html src/ --fix",
    "lint:prettier": "prettier \"src/**/*.{js,ts,tsx,html}\" --check",
    "format:prettier": "prettier \"src/**/*.{js,ts,tsx,html}\" --write"
  },
  "dependencies": {
    "@apollo/client": "^3.9.5",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@microlink/react-json-view": "^1.23.0",
    "@mui/icons-material": "^5.15.10",
    "@mui/material": "^5.15.10",
    "@mui/x-date-pickers": "^6.19.4",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.4",
    "graphql": "^16.8.1",
    "graphql-ws": "^5.15.0",
    "license-report": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "6.22.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^3.5.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/client-preset": "4.2.2",
    "@graphql-codegen/typescript": "^4.0.4",
    "@magidoc/cli": "^4.1.4",
    "@storybook/addon-actions": "^7.6.16",
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/builder-webpack5": "^7.6.16",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/node-logger": "^7.6.16",
    "@storybook/preset-create-react-app": "^7.6.16",
    "@storybook/react": "^7.6.16",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/chai": "^4.3.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chai": "^5.1.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-react": "^7.33.2",
    "http-proxy-middleware": "^2.0.6",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "prop-types": "^15.8.1",
    "sinon": "17.0.1",
    "storybook-dark-mode": "^3.0.3",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^36.0.0",
    "tsc-alias": "^1.8.8",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json}": [
      "eslint",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --config .stylelintrc.json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "src/reportWebVitals.ts",
      "src/index.tsx",
      "src/assets",
      "src/API",
      "src/types"
    ]
  }
}